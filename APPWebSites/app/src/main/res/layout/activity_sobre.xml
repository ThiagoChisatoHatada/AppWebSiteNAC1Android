<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SobreActivity">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Voltar"
        android:id="@+id/btvoltar">
    </Button>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:gravity="center"
        android:layout_marginTop="50sp"
        android:text="Thiago Chisato Hatada"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:gravity="center"
        android:layout_marginTop="100sp"
        android:text="Projeto AppWebSites"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:gravity="center"
        android:layout_marginTop="150sp"
        android:text="MercadoLivre: 7 cliques"
        android:id="@+id/mercadoLivre"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:gravity="center"
        android:layout_marginTop="200sp"
        android:text="Buscape: 7 cliques"
        android:id="@+id/buscaPe"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:gravity="center"
        android:layout_marginTop="250sp"
        tools:text="eBay: 7 cliques"
        android:id="@+id/eBay"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:gravity="center"
        android:layout_marginTop="300sp"
        tools:text="WebMotors: 7 cliques"
        android:id="@+id/webMotors"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:gravity="center"
        android:layout_marginTop="350sp"
        tools:text="Magalu: 7 cliques"
        android:id="@+id/magalu"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:gravity="center"
        android:layout_marginTop="400sp"
        tools:text="Netshoes: 7 cliques"
        android:id="@+id/netShoes"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:gravity="center"
        android:layout_marginTop="450sp"
        tools:text="Americanas: 7 cliques"
        android:id="@+id/americanas"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:gravity="center"
        android:layout_marginTop="500sp"
        tools:text="SubMarino: 5 cliques"
        android:id="@+id/subMarino"/>

    <Button
        android:id="@+id/btApagar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="550sp"
        android:text="Apagar"></Button>
</FrameLayout>